{
  "name": "backosu",
  "version": "2.0.0",
  "description": "backup all your songs into a text file.",
  "main": "main.js",
  "author": "KarboX@KarboXXX <karbox@osubackup.com>",
  "repository": "https://github.com/KarboXXX/backosu/",
  "bugs": "https://github.com/KarboXXX/backosu/issues",
  "license": "MIT",
  "dependencies": {
    "dialog": "^0.3.1",
    "diff": "^5.0.0",
    "file-dialog": "^0.0.8",
    "https": "^1.0.0",
    "os": "^0.1.2",
    "process": "^0.11.10",
    "q": "^1.5.1",
    "readline": "^1.3.0",
    "shell": "^0.8.6",
    "sweetalert2": "^11.3.4",
    "win-select-folder": "^1.0.1"
  },
  "devDependencies": {
    "electron": "^19.0.3",
    "electron-builder": "^22.14.5"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "deps": "electron-builder install-app-deps",
    "linux-compile": "electron-builder --linux --x64"
  },
  "postinstall": "electron-builder install-app-deps",
  "build": {
    "productName": "backosu",
    "linux": {
      "category": "Utility, Game",
      "target": [
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.utility"
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  }
}
