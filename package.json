{
  "name": "osubackup",
  "version": "1.0.0",
  "description": "backup all your songs into a text file.",
  "main": "main.js",
  "author": "KarboX@KarboXXX <karbox@osubackup.com>",
  "bugs": "https://github.com/KarboXXX/osubackup/issues",
  "license": "MIT",
  "dependencies": {
    "dialog": "^0.3.1",
    "fast-diff": "^1.2.0",
    "file-dialog": "^0.0.8",
    "https": "^1.0.0",
    "os": "^0.1.2",
    "process": "^0.11.10",
    "readline": "^1.3.0",
    "shell": "^0.8.6",
    "sweetalert2": "^11.3.4"
  },
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.5"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "deps": "electron-builder install-app-deps",
    "linux-compile": "electron-builder --linux --x64"
  },
  "postinstall": "electron-builder install-app-deps",
  "build": {
    "productName": "OsuBackUp",
    "linux": {
      "category": "Utility, Game",
      "target": [
        "deb",
        "AppImage",
        "tar.xz"
      ]
    },
    "mac": {
      "category": "public.app-category.utility"
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  }
}
